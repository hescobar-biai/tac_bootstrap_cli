# Orchestrator Frontend Environment Variables
# Copy this file to .env and adjust values for your environment.

# REST API base URL for the orchestrator backend (Task 12)
VITE_API_URL=http://localhost:8000

# WebSocket URL for real-time agent status updates
VITE_WS_URL=ws://localhost:8000

# Development server port
VITE_PORT=5173

# Fallback polling interval in milliseconds (used when WebSocket connection fails)
VITE_POLLING_INTERVAL=30000
